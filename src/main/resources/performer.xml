<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">
<bean id="guiter" class="com.zensar.spring.instruments.Guiter"></bean>
<bean id="piano" class="com.zensar.spring.instruments.Piano"></bean>
<bean id="keyboard" class="com.zensar.spring.instruments.Keyboard"></bean>
<bean id="kalyani" class="com.zensar.spring.performers.Instrumentalist">


<property name="instrumentSongMap">
<map>
<entry key-ref="piano" value="dheera"></entry>
<entry key-ref="guitar" value="dheera1"></entry>
<entry key-ref="keyboard" value="majli"></entry>
</map>
</property>
</bean>

</beans>
